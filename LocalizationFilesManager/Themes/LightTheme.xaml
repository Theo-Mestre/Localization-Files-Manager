<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BackgroundBrush" Color="#FFEFEFF5"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="#d2d3db"/>

    <SolidColorBrush x:Key="ButtonBrush" Color="#d2d3db"/>
    <SolidColorBrush x:Key="ButtonHoveredBrush" Color="#FFC2C3CA"/>
    <SolidColorBrush x:Key="ButtonClickedBrush" Color="#FFAAABB3"/>

    <SolidColorBrush x:Key="TextBrush" Color="#FF404040"/>

    <SolidColorBrush x:Key="TopBarBackgroundBrush" Color="#d2d3db"/>
    <SolidColorBrush x:Key="TopBarForegroundBrush" Color="#FF404040"/>
    <SolidColorBrush x:Key="TopBarForegroundHovered" Color="#FF929292"/>

</ResourceDictionary>